   "source": [
    "import numpy as np\n",
    "\n",
    "def ReLU(x):\n",
    "    # np.maximum compares array of left against right, returning array of the max\n",
    "        # i.e., [0, 0, 0] vs [-1,2,3] is [0,2,3]\n",
    "    return np.maximum(0, x)\n",
    "\n",
    "inputs = [-3,-2,-1,1,2,3]\n",
    "\n",
    "print(ReLU(inputs))"
   ]
  }
